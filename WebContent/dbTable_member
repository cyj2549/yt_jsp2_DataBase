CREATE TABLE MEMBER (
id VARCHAR(20) PRIMARY KEY,
pass1 VARCHAR(20),
email VARCHAR(50),
tel VARCHAR(20),
hobby VARCHAR(60), 
job VARCHAR(15),
age VARCHAR(10),
info VARCHAR(500)
);


